package org.cts.test;

public class scan {
	public static void main(String[] args) {

		byte age = 22;
		System.out.println("My age is:" + age);

		int regno = 987654321;
		System.out.println("my reg no is :" + regno);

		short id = 4444;
		System.out.println("my id is :" + id);

		char initial = 'A';
		System.out.println("my initial is:" + initial);

		float sal = 25.5f;
		System.out.println("my sal is:" + sal);

		double avg = 22;
		System.out.println("my avg is:" + avg);

		long phnno = 9876543210l;
		System.out.println("my phn no is:" + phnno);

		String name = "Greens";
		System.out.println("my name is:" + name);

		boolean isEmployee = true;
		System.out.println("i am employee:" + isEmployee);
		System.out.println("Raseem");
		System.out.println("Raseem");
		System.out.println("Raseem");
		System.out.println("Raseem");
		System.out.println("Raseem");

		System.out.println("Raseem");
	}

}
